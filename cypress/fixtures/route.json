{
    "service" : {
        "name": "kong-air-service",
        "protocol": "https",
        "path": "/flights",
        "read_timeout": 60000,
        "retries": 5,
        "host": "api.kong-air.com",
        "connect_timeout": 60000,
        "write_timeout": 60000,
        "port": 443,
        "enabled": true
    },
    "routes" : [
        {
            "name": "route-air-api-v1",
            "path": "/test/api/v1",
            "method": "GET",
            "stripPath": true,
            "tags": "tag1"
        },
        {
            "name": "route-air-api-v2",
            "path": "/test/api/v2",
            "method": "GET",
            "stripPath": true,
            "tags": "tag2"
        },
        {
            "name": "route-air-api-v3",
            "path": ["/test/api/v3","/test/api/v4"],
            "method": "GET",
            "stripPath": false,
            "tags": "tag3"
        }
    ]
}